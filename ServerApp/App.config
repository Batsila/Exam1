<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
                    <serviceDebug includeExceptionDetailInFaults="false" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service name="ServerApp.CommunicationService" behaviorConfiguration="mexBehavior">
             <endpoint name="duplexEndpoint" binding="wsDualHttpBinding"  contract="ServerApp.ICommunicationService"/>
              <endpoint address="mex"
                        binding="mexTcpBinding"
                        contract="IMetadataExchange"
                        behaviourConfiguration="" />
            </service>
      </services>
      <behaviors>
        <serviceBehaviors>
          <behavior name="mexBehavior">
            <serviceMetadata httpGetEnabled="true"/>
          </behavior>
        </serviceBehaviors>
      </behaviors>
    </system.serviceModel>
</configuration>