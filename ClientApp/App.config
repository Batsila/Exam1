<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IServerConnectionService" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:19999/ConnectionService.ServerConnectionService"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServerConnectionService"
        contract="ServerConnectionService.IServerConnectionService"
        name="BasicHttpBinding_IServerConnectionService" />
    </client>
    <services>
      <service name="ServerApp.CommunicationService" behaviorConfiguration="mexBehavior">
        <endpoint address="" binding="wsDualHttpBinding" contract="ServerApp.ICommunicationService"/>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8080/"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="mexBehavior">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>